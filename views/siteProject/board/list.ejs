<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <td style="width: 50px">이름</td>
          <td style="width: 300px">제목</td>
          <td style="width: 300px">날짜</td>
          <td style="width: 100px">수정</td>
          <td style="width: 100px">삭제</td>
        </tr>
      </thead>
      <tbody>
        <%
          for(var i in rows) {
        %>
          <tr>
            <td style="width: 50px"><%=rows[i].name%></td>
            <td style="width: 300px"><a href="/read/<%=rows[i]._id%>"><%=rows[i].title%></a></td>
            <td style="width: 300px"><%=rows[i].date%></td>
            <td style="width: 100px"><a href="/update/<%=rows[i]._id%>">수정</a></td>
            <td style="width: 100px"><a href="/deletes/<%=rows[i]._id%>">삭제</a></td>
          </tr>
          <%  } %>
      </tbody>
    </table>
        <a href="/list/1"><<<</a>
        <a href="/list/<%=page - 1%>">이전</a>
        <%
        for(i = startPage; i <= endPage; i++) {
          if(rows[0].is_delete != 1) {
            if(page == i) { %>
              <a><%=i%></a>
          <%  } else { %>
        <a href="/list/<%=i%>"><%=i%></a>
        <%  }}} %>
        <a href="/list/<%=page + 1%>">다음</a>
        <a href="/list/<%=totalPage%>">>>></a>
  </body>
</html>
